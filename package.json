{
  "name": "api-node-sequalize",
  "version": "1.0.0",
  "description": "A simple project example with node, tests and sequalize orm",
  "main": "./src/app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "local": "nodemon ./src/app.js",
    "swagger-autogen": "node ./src/swagger.js",
    "test": "jest",
    "test:silent": "jest --passWithNoTests --colors --silent --verbose",
    "test:watch": "npm test -- --watch",
    "test:staged": "jest --passWithNoTests",
    "test:ci": "npm run test:silent --coverage --no-cache",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ialbas/api-node-sequalize"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "keywords": [
    "api",
    "rest",
    "jwt",
    "code clean",
    "solid",
    "tdd",
    "jest",
    "postman"
  ],
  "author": "Ialbas Junior",
  "license": "Mit",
  "bugs": {
    "url": "https://github.com/ialbas/api-node-sequalize/issues"
  },
  "homepage": "https://github.com/ialbas",
  "dependencies": {
    "@vscode/sqlite3": "^5.0.8",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "sequelize": "^6.17.0",
    "swagger-autogen": "^2.20.8",
    "swagger-ui-express": "^4.3.0",
    "tedious": "^14.4.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1",
    "standard": "^16.0.4",
    "supertest": "^6.2.2"
  }
}
